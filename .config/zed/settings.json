{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "buffer_font_family": "JetBrainsMonoNL Nerd Font",
  "buffer_font_size": 19,
  "features": {
    "inline_completion_provider": "supermaven"
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "tabs": {
    "activate_on_close": "neighbour",
    "file_icons": true,
    "git_status": true
  },
  "terminal": {
    "toolbar": {
      "title": false
    }
  },
  "theme": "Andromeda",
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "search": {
    "regex": true
  },
  "show_whitespaces": "boundary",
  "ui_font_size": 19,
  "vim": {
    "use_system_clipboard": "on_yank"
  },
  "vim_mode": true
}
